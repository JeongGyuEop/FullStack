public class Opr01_03 {

	public static void main(String[] args) {
		
		// 정수형 변수
		int intVar1 = 10;
		int intVar2 = 20;
		
		// 실수형 변수
		double doubleVar = 5.5;
		
		// 1. 정수형 + 정수형의 덧셈
		int intSum = intVar1 + intVar2; // 10 + 20 = 30
		System.out.println("intSum : " + intSum);

	}

}
