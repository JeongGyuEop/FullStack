
// 주제: 변수를 선언할 때 자료형의 역할에 대해서 살펴보자.

// 자료형의 역할 2가지

public class Data04 {
	public static void main(String[] args) {
		/*
		 *  변수 메모리 선언 문법
		 *  	자료형 변수이름;
		 */
		
		// 소수점이 없는 값 1을 선언한 변수 a에 저장하기 위해서는 
		// int 자료형으로 a라는 이름의 변수를 선언해야 한다.
		int a;
		a = 1;
		
		float m = 2.4;
		
	}
}
